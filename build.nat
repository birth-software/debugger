const std = #import("std");
const Executable = std.build.Executable;
const main = fn() s32 {
    const executable = Executable{
        .target = .{
            .cpu = .x86_64,
            .os = .linux,
            .abi = .gnu,
        },
        .main_source_path = "src/main.nat",
    };

    std.print(bytes = "Hello from the builder\n");
    return 0;
}
